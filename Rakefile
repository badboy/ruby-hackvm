require 'rubygems'
require 'echoe'

Echoe.new('ruby-hackvm', '0.1.0') do |p|
  p.description    = "a virtual machine for hackers."
  p.url            = "http://github.com/BadBoy/ruby-hackvm"
  p.author         = "BadBoy_"
  p.email          = ""
  p.ignore_pattern = ["tmp/*", "script/*"]
  p.development_dependencies = []
end

